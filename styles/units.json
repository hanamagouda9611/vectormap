{
    "layers": [
      {
        "id": "units-symbol",
        "type": "circle",
        "source": "map-tiles",
        "source-layer": "UNITS_LITE_V1_13",
        "minzoom": 12,
        "maxzoom": 24,
        "paint": {
          "circle-color": "#487bb6",
          "circle-stroke-color": "#325780",
          "circle-stroke-width": 1,
          "circle-radius": 4
        }
      },
      {
        "id": "level-label",
        "type": "symbol",
        "source": "map-tiles",
        "source-layer": "UNITS_LITE_V1_13",
