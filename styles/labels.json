{
    "layers": [
      {
        "id": "country_labels",
        "type": "symbol",
        "source": "map-tiles",
        "source-layer": "COUNTRY_BOUNDARY_V1_1_POINTS",
        "filter": ["==", "$type", "Point"],
        "minzoom": 3,
        "maxzoom": 4,
        "layout": {
            "text-field": ["get", "STATE"],
            "text-size": 12,
            "text-font": ["Arial Unicode MS Bold"],
            "text-variable-anchor": ["center"], 
            "text-allow-overlap": false,
            "text-offset": [0, 0]
        },
        "paint": {
            "text-color": "#656565" 
        }
      },
      {
        "id": "state_labels",
        "type": "symbol",
        "source": "map-tiles",
        "source-layer": "STATE_BOUNDARY_V1_1_POINTS",
        "minzoom": 4,
        "maxzoom": 7,
        "filter": [
          "all",
          ["==", "$type", "Point"]
        ],
        "layout": {
          "text-field": ["get", "STATE"],
          "text-font": ["Arial Unicode MS Bold"],
          "text-size": {
            "stops": [[4, 8], [8, 12]]
          },
          "visibility": "visible",
          "text-variable-anchor": ["top"], 
          "text-transform": "uppercase",
          "symbol-z-order": "source",
          "symbol-avoid-edges": false,
          "text-padding": 5,
          "text-allow-overlap": false,
          "text-ignore-placement": false,
          "text-max-width": 10,
          "text-optional": false
          
        },
        "paint": {
          "text-color": "#000000",
          "text-halo-blur": 0,
          "text-halo-color": "hsla(0, 0%, 100%, 0.75)",
          "text-halo-width": 2
        }
      },
      {
        "id": "dist_labels",
